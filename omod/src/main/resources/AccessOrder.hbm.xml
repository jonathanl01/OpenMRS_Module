<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="@MODULE_PACKAGE@">
	
	<class name="AccessOrder" table="access_order">

		<id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
		
		<property name="date" not-null="true" type="java.sql.Date" column="date" />
		
		<property name="order_id" not-null="true" type="int" column="order_id" />
		
		<property name="view_edited" not-null="true" type="char" column="view_edited" />
		
		<property name="user_id" not-null="true" type="int" column="user_id" />
		
		<property name="patient_id" not-null="true" type="int" column="patient_id" />
		
		<property name="location_id" type="int" column="location_id" />
	</class>
</hibernate-mapping>

